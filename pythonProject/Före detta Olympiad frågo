Contestants = int(input("Amount of People: "))
Island = 0
My = 1
Myone = True
ForMy = 0
SecondMy = False
while Contestants > 0:
    if Myone == True:
        My += ForMy
        Myone = False
        SecondMy = True
        Contestants = Contestants - ForMy
    else:
        ForMy += My
        Myone = True
        SecondMy = False
        Contestants = Contestants - ForMy
    Island += 1
print(Island)

bye = int(input("Amount Teabags: "))
Amount = int(input("Amount of People"))

hello = {}
for j in range(bye):
    hello["string{0}".format(j)] = int(input("Write Amount"))

print(hello["strings"])



